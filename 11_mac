alias txmt_text="defaults write com.macromates.textmate OakDefaultLanguage 3130E4FA-B10E-11D9-9F75-000D93589AF6"
alias clean_open_with="/System/Library/Frameworks/ApplicationServices.framework/Versions/A/Frameworks/LaunchServices.framework/Versions/A/Support/lsregister -kill -domain local -domain system -domain user"
alias kickstart="sudo /System/Library/CoreServices/RemoteManagement/ARDAgent.app/Contents/Resources/kickstart  -restart -agent"
alias fdns="sudo dscacheutil -flushcache"
alias safesleepoff="sudo pmset -a hibernatemode 0"
alias safesleepon="sudo pmset -a hibernatemode 3"
alias budt="rsync -avv ~/Library/Application\ Support/Desktoptopia/.Backgrounds/ ~/Desktop\ Pictures/DesktoptopiaBu | grep '.jpg$'"
alias rm_ds="find . -name ".DS_Store" | xargs rm"

alias webmin_start='sudo /opt/local/etc/webmin/start >/dev/null 2>&1 </dev/null'
alias webmin_stop='sudo /opt/local/etc/webmin/stop'

alias macprofile="mate ~/.MacOSX/environment.plist"

